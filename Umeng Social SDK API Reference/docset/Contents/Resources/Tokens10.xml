			<DeclaredIn>UMSocialConfigDelegate.h</DeclaredIn>			
			
			<Declaration>- (BOOL)shouldShareSynchronous</Declaration>
			
			
			<Anchor>//api/name/shouldShareSynchronous</Anchor>
		</Token>
		
